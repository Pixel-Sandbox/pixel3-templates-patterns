{
  "name": "patterns-templates",
  "type": "module",
  "version": "0.0.22",
  "private": true,
  "scripts": {
    "dev": "vite --port 8800 --open",
    "build": "vite build",
    "typecheck": "vue-tsc",
    "preview": "vite preview",
    "deploy:cf": "vite build && wrangler pages deploy dist --project-name=mekari-pattern-templates",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vercel-prebuild": "vercel build --prod && vercel deploy --prebuilt --prod",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@mekari/pixel3": "latest",
    "@mekari/pixel3-postcss": "latest",
    "@mekari/pixel3-styled-system": "latest",
    "@mekari/pixel3-utils": "latest",
    "@vueuse/core": "^10.7.2",
    "@neodrag/vanilla": "^2.3.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "@mekari/pixel3-theme": "latest",
    "@mekari/pixel3-cli": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.56.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.0.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.6",
    "vue-tsc": "^1.8.5"
  }
}